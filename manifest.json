{
    "manifest_version": 3,
    "name": "Random Bookmark Opener",
    "version": "1.0",
    "permissions": [
      "bookmarks",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Open Random Bookmark"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  